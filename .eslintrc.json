{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": "warn",
    "import/no-dynamic-require": 0,
    "no-console": 0,
    "global-require": 0
  }
}
